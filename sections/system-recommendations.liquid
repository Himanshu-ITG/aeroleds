              {{ block.settings.content }}   
               <div class="heading_titlecontent">{{ block.settings.heading_titlecontent }}</div>
               <div class="heading_subcontent">  {{ block.settings.heading_subcontent }} </div>
               <div class="approvedcontent">  {{ block.settings.approvedcontent }} </div>
               <div class="partNumbercontent">  {{ block.settings.partNumbercontent }} </div> 
              </div>

