              </div>
                    </div>
       
                    
   
          {% else %}
            <div class="led_grid-img">
                          <div class="led_grid-img-text">
                      
                      <div class="led_inline">
                  {{ block.settings.inline_text }}
                </div>
                    <div class="led_series">
