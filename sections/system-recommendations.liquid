                        <img src="{{ block.settings.image_par | img_url: 'master' }}" alt="{{ shop.name }}">
                      </div>
                    {% endif %}
                {% if block.settings.image_par2 != blank %}
                  <div class="led_par">
                        <img src="{{ block.settings.image_par2 | img_url: 'master' }}" alt="{{ shop.name }}">
                  </div>
                          {% endif %}
              </div>
            </div>
          {% endif %}
            </div>
          <div class="led_heading">
            <h4>{{ block.settings.heading }}</h4>
          </div>
          {% if block.settings.image != blank %}
            <div class="led_main-img">
              <img src="{{ block.settings.image | img_url: 'master' }}" alt="{{ shop.name }}">
            </div>
          {% endif %}
          {% if block.settings.image2 != blank %}
            <div class="led_main-img bottom-img">
              <img src="{{ block.settings.image2 | img_url: 'master' }}" alt="{{ shop.name }}">
            </div>
          {% endif %}
          <div class="led_content">
            {% if block.settings.choose_type %}
            <div class="led-choose-title">
              <p>{{ block.settings.led-choose-title }}</p>
            </div>
            <div class="led-choose-light">
              {% if block.settings.certified_img != blank %}
              <div class="certified_img led-tab active" data-get="certified">
                <img src="{{ block.settings.certified_img | img_url: 'master' }}" alt="{{ shop.name }}">
              </div>
              {% endif %}
              {% if block.settings.experimental_img != blank %}
              <div class="experimental_img led-tab" data-get="experimental">
                <img src="{{ block.settings.experimental_img | img_url: 'master' }}" alt="{{ shop.name }}">
              </div>
              {% endif %}
            </div>
