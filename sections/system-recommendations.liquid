                      <span>{{ block.settings.title }}</span>
                    </div>
           <div class="led_image-par">
                    {% if block.settings.image_par != blank %}
                      <div class="led_par">
                        <img src="{{ block.settings.image_par | img_url: 'master' }}" alt="{{ shop.name }}">
                      </div>
                    {% endif %}
                {% if block.settings.image_par2 != blank %}
                  <div class="led_par">
