        "block_FE9H8W": {
          "type": "block",
          "settings": {
            "archived_img": "shopify://shop_images/PAR56_Left.png",
            "heading": "Sunspot 56-4000",
            "faq_inner_heading": "<h2><a href=\"/products/sunspot-56-4000-led-landing-light\" title=\"SUNSPOT 56-4000 LED Landing Light\">Sunspot 56-4000</a></h2>",
