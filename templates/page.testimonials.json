    "rich_text_tM6AGR": {
      "type": "rich-text",
      "blocks": {
        "text_AhWi7q": {
          "type": "text",
          "settings": {
            "text": "<h5>I have flown in dark twilight several times. – I fly in busy airspace and keep the wigwags on most of the time for extra visibility.</h5>"
